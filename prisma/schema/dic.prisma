model System {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @ignore @default(now())
  updatedAt DateTime @ignore @updatedAt

  label String @unique
  value String @unique
  apps  App[]

	@@index([label, value])
}

model Platform {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @ignore @default(now())
  updatedAt DateTime @ignore @updatedAt

  label String @unique
  value String @unique
  apps  App[]

  @@index([label, value])
}
